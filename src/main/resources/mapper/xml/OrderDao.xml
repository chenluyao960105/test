<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aaa.test.dao.OrderDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.aaa.test.entity.Order">
        <id column="order_id" property="orderId" />
        <result column="order_time" property="orderTime" />
        <result column="order_cus" property="orderCus" />
        <result column="order_price" property="orderPrice" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        order_id AS orderId, order_time AS orderTime, order_cus AS orderCus, order_price AS orderPrice
    </sql>

</mapper>
